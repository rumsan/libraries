{
	"name": "prisma-db",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/prisma-db/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/prisma-db",
				"tsConfig": "libs/prisma-db/tsconfig.lib.json",
				"packageJson": "libs/prisma-db/package.json",
				"main": "libs/prisma-db/src/index.ts",
				"assets": ["libs/prisma-db/*.md"]
			}
		},
		"publish": {
			"command": "node tools/scripts/publish.mjs prisma-db {args.ver} {args.tag}",
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/prisma-db/**/*.ts",
					"libs/prisma-db/package.json"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/prisma-db/jest.config.ts"
			}
		}
	},
	"tags": []
}
